---
export interface Props {
  name: string
  w?: number
  h?: number
  additionalClass?: string
}

const { w = 11, h = 11, name, additionalClass = '', ...attrs } = Astro.props
---

<img
  class={`w-${w} h-${h} rounded-full ${additionalClass} `}
  src={`./icons/${name}.svg`}
  alt={`${name}Icon`}
  {...attrs}
/>
